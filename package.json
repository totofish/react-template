{
  "name": "react-template",
  "version": "2.0.0",
  "description": "React Template",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/totofish/react-template.git"
  },
  "scripts": {
    "dev": "gulp webpack-dev-server",
    "release": "gulp && webpack --env.production && gulp manifest && node server.js --env.production",
    "addlibs": "yarn add git+https://github.com/totofish/react-libs.git",
    "cp": "yarn run cp:console && yarn run cp:es5-sham && yarn run cp:es5-shim && yarn run cp:es6-promise && yarn run cp:es6-shim && yarn run cp:fetch && yarn run cp:xhr",
    "cp:console": "cp -f node_modules/console-polyfill/index.js src/assets/scripts/console-polyfill.js",
    "cp:es5-sham": "cp -f node_modules/es5-shim/es5-sham.min.js src/assets/scripts",
    "cp:es5-shim": "cp -f node_modules/es5-shim/es5-shim.min.js src/assets/scripts",
    "cp:es6-promise": "cp -f node_modules/es6-promise/dist/es6-promise.min.js src/assets/scripts",
    "cp:es6-shim": "cp -f node_modules/es6-shim/es6-shim.min.js src/assets/scripts",
    "cp:fetch": "cp -f node_modules/fetch-polyfill/fetch.js src/assets/scripts",
    "cp:xhr": "cp -f node_modules/xhr-polyfill/dist/xhr-polyfill.js src/assets/scripts"
  },
  "author": "totofish",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-manifest": "^0.1.1",
    "gulp-notify": "^3.0.0",
    "gulp-open": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-uglify": "^2.0.1",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.5.0",
    "opener": "^1.4.3",
    "parallelshell": "^2.0.0",
    "portfinder": "^1.0.13",
    "postcss-loader": "^1.3.1",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "console-polyfill": "^0.2.3",
    "domready": "^1.0.8",
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.3",
    "fetch-polyfill": "^0.8.2",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-libs": "git+https://github.com/totofish/react-libs.git",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-scroll": "^0.4.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-saga": "^0.14.3",
    "xhr-polyfill": "^0.1.8"
  }
}
